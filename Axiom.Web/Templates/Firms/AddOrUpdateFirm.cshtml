                                @*<li ng-show="Firmform.Email.$valid">Please enter valid email.</li>*@
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="row  mb-10">
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary required pr-0">Name</label>
