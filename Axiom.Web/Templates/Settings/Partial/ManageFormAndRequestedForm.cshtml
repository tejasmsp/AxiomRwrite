<div class="modal-dialog modal-lg">
    <div class="modal-content">
        <div class="modal-header bg-main">
            <button type="button" class="close" data-dismiss="modal" title="Close">&times;</button>
            <h6 class="modal-title" ng-show="parentForm == 'Location'"><i class="icon-plus-circle2 mr-5"></i>{{title}} ( Location Code: {{parentLocationObj.LocID}} | Name:{{parentLocationObj.Name}})</h6>
            <h6 class="modal-title" ng-show="parentForm == 'Attorney'"><i class="icon-plus-circle2 mr-5"></i>{{title}} ( Attorney ID: {{objAttorneyForm.AttyID}} | Name:{{objAttorneyForm.Name}})</h6>
            <h6 class="modal-title" ng-show="parentForm == 'Firms'"><i class="icon-plus-circle2 mr-5"></i>{{title}} ( Firm ID: {{objFirmForm.FirmID}} | Name:{{objFirmForm.Name}})</h6>
        </div>
        <div class="modal-body">
            <div class="modal-container">
                <div class="table-bordered p-10 pnl-back-grey datatable-scroll mb-10 ">
                    <h6 class="h6 heading-divided">Select Document</h6>
                    <div data-angular-treeview="false" style="max-height:200px; overflow-y:auto; padding:0px;"
                         data-tree-id="mytree"
                         data-tree-model="FormsAndDirectoryList"
                         data-node-id="title"
                         data-node-label="title"
                         data-node-expanded="isExpanded"
                         data-node-children="children"
                         class="panel-body">
                    </div>
                </div>
                <div class="table-bordered p-10 pnl-back-grey datatable-scroll">
                    <div class="table-responsive table-grid">
                        <table class="table table-bordered table-framed" id="tblLocationForm"></table>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal-footer">
            <button title="Close" type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        </div>
    </div>
</div>
