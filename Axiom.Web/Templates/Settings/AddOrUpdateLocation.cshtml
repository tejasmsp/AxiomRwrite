<div class="page-content cls-base" ng-controller="LocationAddUpdateController">
    <div class="panel panel-white">
        <div class="panel-heading">
            <h4 class="panel-title text-semibold">
                <i class="icon-file-plus mr-5 fs-19"></i>
                {{isEdit? 'Edit Location': 'Add Location'  }}

            </h4>

        </div>
        <div class="panel-body pl-10 pr-10">
            <form class="form-horizontal" name="Locationform" novalidate>
                <div class="row">
                    <div id="messages" class="col-md-12 pl-20 pr-20">
                        <div class="alert alert-danger alert-dismissible validationErrors" ng-show="((Locationform.$submitted || EnableGenerteCodeValidation) && !Locationform.$valid)  ">
                            <ul>
                                <li ng-if="Locationform.LocID.$error.required && !EnableGenerteCodeValidation">Location ID is required.</li>
                                <li ng-if="Locationform.Name1.$error.required">Location Name is required.</li>
                                <li ng-if="Locationform.Email.$error.email">Invalid Email.</li>
                                <li ng-if="Locationform.FirmID.$error.required">Firm ID is required.</li>
                                <li ng-if="Locationform.BillingLocId.$error.required">Billing Location ID is required.</li>
                                <li ng-if="Locationform.FilmsLocId.$error.required">Flims Location ID is required.</li>
                            </ul>
                        </div>
                    </div>
                </div>

                <div class="row mb-10">
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary required pr-0">Location Name:</label>
                        <div class="col-sm-9">
                            <input type="text" name="Name1" class="form-control" maxlength="150" ng-model="LocationObj.Name1" required />
                        </div>
                    </div>
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary required pr-0">Location ID:</label>
                        <div class="col-sm-4">
                            <input type="text" name="LocID" class="form-control" maxlength="15" ng-model="LocationObj.LocID" readonly required="required">
                        </div>
                        <div class="col-sm-5 no-padding" ng-if="!isEdit">
                            <input class="btn btn-group-sm" type="button" ng-click="GenerateNewLocationCode(Locationform)" value="Generate" />
                        </div>
                    </div>
                </div>

                <div class="row mb-10">
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary">Doctor Name:</label>
                        <div class="col-sm-9">
                            <input type="text" name="Name2" class="form-control" maxlength="150" ng-model="LocationObj.Name2" />
                        </div>
                    </div>
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0">Street 1:</label>
                        <div class="col-sm-9">
                            <input type="text" name="Street1" class="form-control" maxlength="100" ng-model="LocationObj.Street1">
                        </div>
                    </div>
                </div>

                <div class="row mb-10">
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0">Street 2:</label>
                        <div class="col-sm-9">
                            <input type="text" name="Street2" class="form-control" maxlength="100" ng-model="LocationObj.Street2" />
                        </div>
                    </div>
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary">City:</label>
                        <div class="col-sm-9">
                            <input type="text" name="City" class="form-control" maxlength="50" ng-model="LocationObj.City" />
                        </div>
                    </div>
                </div>

                <div class="row mb-10">
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0">State:</label>
                        <div class="col-sm-9">
                            <select name="State" ng-model="LocationObj.State" class="form-control" ng-options="item.StateId as item.StateName for item in Statelist">
                                <option value="">--Select--</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0">Zip:</label>
                        <div class="col-sm-9">
                            <input type="text" name="Zip" class="form-control" maxlength="10" ng-model="LocationObj.Zip" />
                        </div>
                    </div>
                </div>

                <div class="row mb-10">
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0">Department:</label>
                        <div class="col-sm-9">
                            <select name="Dept" ng-model="LocationObj.Dept" class="form-control" ng-options="item.Descr as item.Descr for item in Departmentlist">
                                <option value="">--Select--</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0">Email:</label>
                        <div class="col-sm-9">
                            <input type="email" name="Email" class="form-control" maxlength="200" ng-model="LocationObj.Email" />
                        </div>
                    </div>
                </div>

                <div class="row mb-10">
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0">Phone No.:</label>
                        <div class="col-sm-3">
                            <input type="text" name="AreaCode1" class="form-control" placeholder="Area Code" integer-Only="" maxlength="4" ng-model="LocationObj.AreaCode1" />
                        </div>
                        <div class="col-sm-6 pl-0">
                            <input type="text" name="PhoneNo1" class="form-control" placeholder="Phone Number" integer-Only="" maxlength="12" ng-model="LocationObj.PhoneNo1" />
                        </div>
                    </div>
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0">Phone No. 2:</label>
                        <div class="col-sm-3">
                            <input type="text" name="AreaCode2" class="form-control" placeholder="Area Code" integer-Only="" maxlength="4" ng-model="LocationObj.AreaCode2" />
                        </div>
                        <div class="col-sm-6 pl-0">
                            <input type="text" name="PhoneNo2" class="form-control" placeholder="Phone Number" integer-Only="" maxlength="12" ng-model="LocationObj.PhoneNo2" />
                        </div>
                    </div>
                </div>

                <div class="row mb-20">
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0">Fax No.:</label>
                        <div class="col-sm-3">
                            <input type="text" name="AreaCode3" class="form-control" placeholder="Area Code" integer-Only="" maxlength="4" ng-model="LocationObj.AreaCode3" />
                        </div>
                        <div class="col-sm-6 pl-0">
                            <input type="text" name="FaxNo" class="form-control" placeholder="Fax Number" integer-Only="" maxlength="12" ng-model="LocationObj.FaxNo" />
                        </div>
                    </div>
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0">Contact:</label>
                        <div class="col-sm-9">
                            <input type="text" name="Contact" class="form-control"  maxlength="64" ng-model="LocationObj.Contact" />
                        </div>
                    </div>
                </div>

                <div class="row mb-10">
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-5 pt-0">Records that can be Requested:</label>
                        <div class="col-sm-9">
                            <div class="bx-grey pb-5 pr-5 col-sm-12" style="min-height:301px;">
                                <ul class="borderless pt-0 pl-0">
                                    <li class="li-checkbox col-sm-4 pl-0" ng-repeat="item in ReccanRequestedOptions">
                                        <label class="lbl-checkbox bg-white">
                                            <input name="ReccanRequestedgrp" type="checkbox" data-ng-model="item.IsChecked" data-ng-value="true"> {{item.Titile}}
                                        </label>
                                    </li>
                                </ul>
                                <div class="col-sm-12 mb-15"></div>
                                <ul class="borderless pt-0 pl-0">
                                    <li class="li-checkbox col-sm-6 pl-0"><label class="lbl-checkbox bg-white"><input name="CopyService" type="checkbox" data-ng-model="LocationObj.CopyService" data-ng-value="true"> Copy Service</label></li>
                                    <li class="li-checkbox col-sm-6 pl-0"><label class="lbl-checkbox bg-white"><input name="FeeAmountSendRequest" type="checkbox" data-ng-model="LocationObj.FeeAmountSendRequest" data-ng-value="true"> Fee Amount to Send with Request</label></li>
                                    <li class="li-checkbox col-sm-6 pl-0"><label class="lbl-checkbox bg-white"><input name="ReqAuthorization" type="checkbox" data-ng-model="LocationObj.ReqAuthorization" data-ng-value="true"> Requires Original Authorization</label></li>
                                    <li class="li-checkbox col-sm-6 pl-0"><label class="lbl-checkbox bg-white"><input name="LocSpecificAuthorization" type="checkbox" data-ng-model="LocationObj.LocSpecificAuthorization" data-ng-value="true"> Location Specific Authorization</label></li>
                                    <li class="li-checkbox col-sm-6 pl-0"><label class="lbl-checkbox bg-white"><input name="LinkRequest" type="checkbox" data-ng-model="LocationObj.LinkRequest" data-ng-value="true"> Location Specific Requests</label></li>
                                </ul>
                            </div>
                        </div>
                    </div>

                    <div class="col-md-6">
                        <div class="row no-margin mb-20">
                            <label class="col-sm-3 control-label lbl-primary pr-5 pt-0">Send Request:</label>
                            <div class="col-sm-9">
                                <div class="bx-grey pb-5 pr-5 col-sm-12">
                                    <ul class="borderless pt-0 pl-0">
                                        <li class="li-checkbox col-sm-4 pl-0" ng-repeat="item in SendRequestOptions">
                                            <label class="lbl-checkbox bg-white">
                                                <input name="SendRequestgrp" type="checkbox" data-ng-model="item.IsChecked" data-ng-value="true"> {{item.Titile}}
                                            </label>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="row no-margin mb-20">
                            @*<label class="col-sm-3 control-label lbl-primary pr-5 pt-0">Request Sent:</label>
                            <div class="col-sm-9">
                                <div class="bx-grey pb-5 pr-5 col-sm-12">
                                    <ul class="borderless pt-0 pl-0">
                                        <li class="li-checkbox col-sm-4 pl-0" ng-repeat="item in RequestSentOptions">
                                            <label class="lbl-checkbox bg-white"><input name="RequestSentgrp" type="radio" data-ng-model="item.IsChecked" ng-click="RadioCheckChange(item, RequestSentOptions)" data-ng-value="true"> {{item.Titile}}</label>
                                        </li>
                                    </ul>
                                </div>
                            </div>*@
                        </div>
                        <div class="row no-margin">
                            <label class="col-sm-3 control-label lbl-primary pr-5 pt-0">Location Options:</label>
                            <div class="col-sm-9">
                                <div class="bx-grey pb-5 pr-5 col-sm-12">
                                    <label class="col-sm-6 pl-0 pr-0 pb-0 mb-0"><label class="lbl-checkbox bg-white"> <input ng-model="LocationObj.IsDisable" name="IsDisable" type="checkbox"> Disable This Location</label></label>
                                    <label class="col-sm-6 mb-0 pb-0"><label class="lbl-checkbox bg-white"><input ng-model="LocationObj.isCommonLocation" name="isCommonLocation" type="checkbox"> Common Location</label></label>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>

                <div class="row mb-10">
                    <div class="col-md-6 mt-10" ng-if="LocationObj.CopyService">
                        <label class="col-sm-3 control-label lbl-primary pr-0 required">Firm ID:</label>
                        @*<div class="col-sm-9">
            <input type="text" class="form-control" ng-model="LocationObj.FirmID" name="FirmID" ng-required="LocationObj.CopyService" maxlength="8">
        </div>*@
                        <div class="col-sm-9 display-inline-flex pl-0 " >
                            <span class="col-sm-3">
                                <input type="text" ng-model="LocationObj.FirmID" maxlength="10" name="FirmID" placeholder="Firm ID" class="form-control" readonly="readonly" required>
                            </span>
                            <span class="col-sm-9">
                                <input type="text" ng-model="LocationObj.FirmName" maxlength="50" name="FirmName" placeholder="Firm Name" class="form-control" readonly="readonly">
                            </span>
                            <a class="ico_btn cursor-pointer pb-0" ng-click="FirmSearch($event)" title="Search Firm"> <i class="icon-search4"></i></a>
                        </div>



                        
                    </div>
                    <div class="col-md-6 mt-10" ng-if="LocationObj.FeeAmountSendRequest">
                        <label class="col-sm-3 control-label lbl-primary pr-0 required">Fee Amount:</label>
                        <div class="col-sm-9">
                            <input type="text" integer-Only=""  class="form-control" ng-model="LocationObj.FeeAmount" name="FeeAmount" maxlength="8">
                        </div>
                    </div>
                    <div class="col-md-6 mt-10" ng-if="LocationObj.LocSpecificAuthorization">
                        <label class="col-sm-3 control-label lbl-primary pr-0 required">Forms:</label>
                        <div class="col-sm-9">
                            <button class="btn btn-group-sm" ng-click="OpenAddForms(false)" title="Manage Forms"> <i class="icon-plus-circle2 mr-5"></i>Manage Forms</button>
                        </div>
                    </div>
                    <div class="col-md-6 mt-10" ng-if="LocationObj.LinkRequest">
                        <label class="col-sm-3 control-label lbl-primary pr-0">Request Forms:</label>
                        <div class="col-sm-9">
                            <button class="btn btn-group-sm" ng-click="OpenAddForms(true)" title="Manage Request Forms"> <i class="icon-plus-circle2 mr-5"></i>Manage Request Forms</button>
                        </div>
                    </div>
                </div>

                <div class="row mb-10">
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0">Warning:</label>
                        <div class="col-sm-9">
                            <textarea class="form-control" ng-model="LocationObj.Warning" name="Warning" rows="4" maxlength="256" style="resize:none;"> </textarea>
                        </div>
                    </div>
                    <div class="col-md-6">

                        <div class="col-sm-3">
                            <div class="col-sm-6">
                                <label class="control-label lbl-primary pr-0">Notes:</label>
                            </div>
                            <div class="col-sm-6 text-right pt-30 pr-0">
                                <i class="icon-clipboard3 position-left cursor-pointer" title="Click here to view notes" ng-click="openLocationNoteClick();"></i>
                            </div>
                        </div>

                        <div class="col-sm-9">
                            <textarea class="form-control" id="Notes" name="Notes" rows="4" maxlength="1000" style="resize:none"> </textarea>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pt-0">Linked Locations</label>
                        <div class="col-sm-9">
                            <div class="bx-grey pb-5 pr-5 col-sm-12">
                                <ul class="borderless pt-0 pl-0">
                                    <li class="li-checkbox col-sm-4 pl-0" ng-repeat="item in LinkedLocationOptions">
                                        <label class="lbl-checkbox">
                                            <input name="LinkedLocationgrp" type="checkbox" data-ng-model="item.IsChecked" data-ng-value="true"> {{item.Titile}}
                                        </label>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary">Authorization Dated within(Days)</label>
                        <label class="col-sm-9"><input type="text" integer-Only=""  name="AuthorizationDays" class="form-control" maxlength="3" ng-model="LocationObj.AuthorizationDays" /></label>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-6 mt-10" ng-if="LinkedLocationOptions[0].IsChecked">
                        <label class="col-sm-3 control-label lbl-primary pr-0 required">
                            Billing Location
                        </label>
                        <div class="col-sm-5 display-inline-flex">
                            <input type="text" name="BillingLocId" class="form-control" maxlength="20" ng-model="LocationObj.BillingLocId"  ng-required="LinkedLocationOptions[0].IsChecked" readonly="readonly" />
                            <a class="ico_btn cursor-pointer mt-1" ng-click="ShowLocationForm('Billing')" title="Search Employee"> <i class="icon-search4"></i></a>
                        </div>
                        <div class="col-sm-4">
                            <input type="text" name="BillingLocName" class="form-control" ng-model="LocationObj.BillingLocName" readonly="readonly" />
                        </div>
                    </div>
                    <div class="col-md-6  mt-10" ng-if="LinkedLocationOptions[2].IsChecked">
                        <label class="col-sm-3 control-label lbl-primary pr-0 required">Films Location</label>
                        <div class="col-sm-5 display-inline-flex">
                            <input type="text" name="FilmsLocId" class="form-control" maxlength="20" ng-model="LocationObj.FilmsLocId" ng-required="LinkedLocationOptions[2].IsChecked"  readonly="readonly"  />
                            <a class="ico_btn cursor-pointer mt-1" ng-click="ShowLocationForm('Films')" title="Search Employee"> <i class="icon-search4"></i></a>
                        </div>
                        <div class="col-sm-4">
                            <input type="text" name="FilmsLocName" class="form-control" ng-model="LocationObj.FilmsLocName" readonly="readonly" />
                        </div>
                    </div>
                </div>

                <div class="row" ng-if="isEdit">
                    <div class="col-md-6 mt-10">
                        <label class="col-sm-3 control-label lbl-primary pr-0">
                            Merge Location(s)
                        </label>
                        <div class="col-sm-9">
                            <button class="btn btn-group-sm" ng-click="OpenMergeLocation()" title="Merge Location"> <i class="icon-merge mr-5"></i>Manage Merge Loaction</button>
                        </div>
                    </div>
                </div>

                <div class="row mt-10">
                    <div class="col-md-12 text-right pr-20">
                        <button type="button" title="Cancel" ui-sref="Location" class="btn btn-default">Cancel</button>
                        <button type="submit" title="Save" ng-click="SaveLocation(Locationform)" class="btn btn-primary mr-5">Save</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
    <!-- Primary modal -->
    <div id="modal_LocationMergedLocation" data-backdrop="static" data-keyboard="false" class="modal fade" static-Include="./Templates/Settings/Partial/MergedLocation.cshtml" ng-controller="MergeLocationController">
    </div>
    <!-- /primary modal -->
    <!-- LocationForm modal -->
    <div id="modal_LocationFormList" data-backdrop="static" data-keyboard="false" class="modal fade" static-Include="./Templates/Settings/Partial/ManageFormAndRequestedForm.cshtml" ng-controller="ManageFormAndRequestedFormController">
    </div>
    <!-- /LocationForm modal -->
    <div id="modal_NotesForm"   data-backdrop="static" data-keyboard="false" class="modal fade" static-Include="./Templates/Settings/Partial/Notes.cshtml">
    </div>


    <div id="modal_Location_Search" class="modal fade" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header bg-main">
                    <button type="button" class="close" data-dismiss="modal" title="Close">&times;</button>
                    <h5 class="modal-title">Search Location</h5>
                </div>
                <div class="modal-body">
                    <div class="modal-container">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="panel-group panel-group-control panel-group-control-right">
                                    <div class="panel panel-white ">
                                        <div class="panel-heading">
                                            <h6 class="panel-title">
                                                <a class="wd-full collapsed" style="width:100%" data-toggle="collapse" href="#SearchPanel">
                                                    <i class="icon-search4 position-left"></i> <b class="text-bold pt-0" style="text-transform:uppercase;">Search</b>
                                                </a>
                                            </h6>
                                        </div>
                                        <div id="SearchPanel" class="panel-collapse collapse">
                                            <div class="panel-body pnl-back-grey" style="padding: 14px 0px 2px 0px !important;">

                                                <div class="row mb-10">
                                                    <div class="col-md-3 hidden">
                                                        <div class="form-group">
                                                            <label class="control-label col-sm-3">Type :</label>
                                                            <div class="col-sm-9">
                                                                <label class="control-label radio-inline"><input type="radio" /> Location</label>
                                                                <label class="control-label radio-inline"><input type="radio" /> Doctor</label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label class="control-label col-sm-3 lh-15">Location ID:</label>
                                                            <div class="col-sm-9">
                                                                <input type="text" name="LocID" class="form-control" maxlength="8" ng-model="LocationSearchObj.LocID" ng-keydown="($event.keyCode === 13) && LocationSearch_Click()" />
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label class="control-label col-sm-3 pt-8">Name:</label>
                                                            <div class="col-sm-9">
                                                                <input type="text" name="Name" class="form-control" maxlength="300" ng-model="LocationSearchObj.Name" ng-keydown="($event.keyCode === 13) && LocationSearch_Click()">
                                                            </div>

                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label class="control-label col-sm-3 lh-15">Phone / Fax :</label>
                                                            <div class="col-sm-9">
                                                                <input type="text" name="PhoneNo" integer-Only=""  class="form-control" maxlength="12" ng-model="LocationSearchObj.PhoneNo" ng-keydown="($event.keyCode === 13) && LocationSearch_Click()" />
                                                            </div>

                                                        </div>
                                                    </div>
                                                   
                                                </div>

                                                <div class="row mb-10">
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label class="control-label col-sm-3 pt-8">Address:</label>
                                                            <div class="col-sm-9">
                                                                <input type="text" name="Address" class="form-control" maxlength="100" ng-model="LocationSearchObj.Address" ng-keydown="($event.keyCode === 13) && LocationSearch_Click()" />
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label class="control-label col-sm-3 pt-8">City:</label>
                                                            <div class="col-sm-9">
                                                                <input type="text" name="City" class="form-control" maxlength="50" ng-model="LocationSearchObj.City" ng-keydown="($event.keyCode === 13) && LocationSearch_Click()">
                                                            </div>

                                                        </div>
                                                    </div>
                                                    <div class="col-md-4 text-right">
                                                        <div class="form-group">
                                                            <label class="control-label col-sm-3 pt-8">State:</label>
                                                            <div class="col-sm-9">
                                                                <select ng-model="LocationSearchObj.State" class="form-control" ng-options="item.StateId as item.StateName for item in Statelist">
                                                                    <option value="">--Select--</option>
                                                                </select>
                                                            </div>

                                                        </div>
                                                    </div>
                                                   
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label class="control-label col-sm-3 pl-0 pt-8">Department:</label>
                                                            <div class="col-sm-9">
                                                                <select name="Department" ng-model="LocationSearchObj.Department" class="form-control" ng-options="item.Department as item.Department for item in Departmentlist">
                                                                    <option value="">--Select--</option>
                                                                </select>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4"></div>
                                                        <div class="col-md-4 ">
                                                            <div class="form-group text-right pr-10">
                                                                <button class="btn btn-primary mr-5" ng-click="LocationSearch_Click()" title="Search">Search</button>
                                                                <button class="btn btn-default" ng-click="ClearLocationSearch_Click()" title="Clear Search Text">Clear</button>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group pb-0 mb-0">
                                    <div class="table-responsive table-grid">
                                        <table class="table table-bordered table-framed" id="tblSearchLocation"></table>
                                    </div>
                                </div>
                            </div>
