
<div class="page-content" ng-controller="AddOrUpdateAttorneyController">
    <div class="panel panel-white">
        <div class="panel-heading">
            <h4 class="panel-title text-semibold">
                <i class="icon-file-text2 mr-10 fs-19"></i>
                {{isEdit? 'Edit Attorney': 'Add Attorney'  }}
            </h4>
        </div>
        <div class="panel-body">
            <form class="form-horizontal" name="Attorneyform" novalidate>
                <div class="row">
                    <div id="messages">
                        <div class="alert alert-danger alert-dismissible validationErrors" ng-show="((Attorneyform.$submitted || EnableGenerteCodeValidation) && !Attorneyform.$valid)">
                            <ul>
                                <li ng-show="Attorneyform.FirmID.$error.required">Please select Firm.</li>
                                <li ng-show="Attorneyform.FirstName.$error.required">Attorney First Name is required.</li>
                                <li ng-show="Attorneyform.LastName.$error.required">Attorney Last Name is required.</li>
                                <li ng-show="Attorneyform.AttorneyID.$error.required">Attorney ID is required.</li>
                                <li ng-show="Attorneyform.Email.$error.email">Invalid Email.</li>
                                <li ng-show="Attorneyform.AssistantContact.$error.email">Invalid Assistant Contact Email.</li>
                                <li ng-show="Attorneyform.ContactEmail.$error.email">Invalid Contact Email.</li>
                                @*<li ng-show="Attorneyform.CompanyNo.$error.required">Invalid Contact Company No.</li>*@
                            </ul>
                        </div>
                    </div>
                </div>

                <div class="bx-grey">
                    <div class="row mb-10">
                        <div class="col-md-6">

                            <label class="col-sm-3 control-label lbl-primary pr-0 mandatoryField">Firm ID:</label>
                            <div class="col-sm-9 display-inline-flex pl-0 " ng-show="!isEdit">
                                <span class="col-sm-3">
                                    <input type="text" ng-model="objAttorney.FirmID" maxlength="20" name="FirmID" placeholder="Firm ID" class="form-control" readonly="readonly" required>
                                </span>
                                <span class="col-sm-9">
                                    <input type="text" ng-model="objAttorney.FirmName" maxlength="50" name="FirmName" placeholder="Firm Name" class="form-control" readonly="readonly">
                                </span>
                                <a class="ico_btn cursor-pointer pb-0" ng-click="FirmSearch($event)" title="Search Firm"> <i class="icon-search4"></i></a>
                            </div>

                            <div class="col-sm-9 pl-0 pr-0" ng-show="isEdit">
                                <span class="col-sm-3">
                                    <input type="text" ng-model="objAttorney.FirmID" maxlength="20" name="FirmID" placeholder="Firm ID" class="form-control" readonly="readonly" required>
                                </span>
                                <span class="col-sm-9">
                                    <input type="text" ng-model="objAttorney.FirmName" maxlength="50" name="FirmName" placeholder="Firm ID" class="form-control" readonly="readonly">
                                </span>
                            </div>

                        </div>
                        <div class="col-md-6">
                            <label class="col-sm-3 control-label lbl-primary pr-0">Street1:</label>
                            <div class="col-sm-9">
                                <input type="text" ng-model="objAttorney.Street1" maxlength="100" name="Street1" placeholder="Street1" class="form-control" readonly="readonly">
                            </div>
                        </div>
                    </div>

                    <div class="row mb-10">
                        <div class="col-md-6">
                            <label class="col-sm-3 control-label lbl-primary pr-0">Street2:</label>
                            <div class="col-sm-9">
                                <input type="text" ng-model="objAttorney.Street2" maxlength="100" name="Street2" placeholder="Street2" class="form-control" readonly="readonly">
                            </div>
                        </div>
                        <div class="col-md-6">
                            <label class="col-sm-3 control-label lbl-primary pr-0 ">City:</label>
                            <div class="col-sm-9">
                                <input type="text" ng-model="objAttorney.City" maxlength="25" name="City" placeholder="City" class="form-control" readonly="readonly">
                            </div>
                        </div>

                    </div>

                    <div class="row mb-10">
                        <div class="col-md-6">
                            <label class="col-sm-3 control-label lbl-primary pr-0">State:</label>
                            <div class="col-sm-9">
                                <input type="text" name="State" ng-model="objAttorney.State" maxlength="20" placeholder="State" class="form-control" readonly="readonly">
                            </div>
                        </div>
                        <div class="col-md-6">
                            <label class="col-sm-3 control-label lbl-primary pr-0">Zip:</label>
                            <div class="col-sm-9">
                                <input type="text" ng-model="objAttorney.Zip" maxlength="20" name="Zip" placeholder="Zip" class="form-control" readonly="readonly">
                            </div>
                        </div>
                    </div>

                    <div class="row mb-10">
                        <div class="col-md-6">
                            <label class="col-sm-3 control-label lbl-primary pr-0">Phone No:</label>
                            <div class="col-sm-3">
                                <input type="text" name="PhoneCode" ng-model="objAttorney.FAreaCode1" maxlength="4" integer-Only="" placeholder="Code" class="form-control" readonly="readonly">
                            </div>
                            <div class="col-sm-6">
                                <input type="text" name="experience-position" ng-model="objAttorney.FPhoneNo" maxlength="10" integer-Only="" placeholder="Phone No" class="form-control" readonly="readonly">
                            </div>
                        </div>
                        <div class="col-md-6">
                            <label class="col-sm-3 control-label lbl-primary pr-0">Fax No:</label>
                            <div class="col-sm-3">
                                <input type="text" name="FaxCode" ng-model="objAttorney.FAreaCode2" maxlength="4" integer-Only="" placeholder="Code" class="form-control" readonly="readonly">
                            </div>
                            <div class="col-sm-6">
                                <input type="text" name="FaxNo" ng-model="objAttorney.FFaxNo" maxlength="10" integer-Only="" placeholder="Fax No" class="form-control" readonly="readonly">
                            </div>
                        </div>
                    </div>
                </div>
                

                <div class="row mb-10 mt-10">
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0 mandatoryField">Attorney ID:</label>
                        <div class="col-sm-7">
                            <input type="text" name="AttorneyID" ng-model="objAttorney.AttyID" maxlength="10" placeholder="Attorney ID" class="form-control" readonly="readonly" ng-required="true">
                        </div>
                        <div class="col-sm-2">
                            <input type="button" name="MakeID" ng-click="GenerateNewAttorneyCode(Attorneyform)" value="Make ID" class="btn btn-group-sm btn-block">
                        </div>
                    </div>
                    @*<div class="col-md-6">
                            <label class="col-sm-3 control-label lbl-primary pr-5 pt-0 mandatoryField">Company No</label>
                            <div class="col-sm-9">
                                <select name="CompanyNo" ng-model="objAttorney.CompanyNo" class="form-control" ng-options="item.CompNo as item.CompNo for item in Companylist" ng-required="true">
                                    <option value="">--Select--</option>
                                </select>
                            </div>
                        </div>*@
                </div>

                <div class="row mb-10">
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0 mandatoryField">First Name :</label>
                        <div class="col-sm-2">
                            <select name="Salutation" ng-model="objAttorney.Salutation" class="form-control" ng-options="item.Value as item.Title for item in  SalutationList"></select>
                        </div>
                        <div class="col-sm-7">
                            <input type="text" name="FirstName" ng-model="objAttorney.FirstName" maxlength="20" placeholder="First Name" class="form-control" required>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0 mandatoryField">Last Name:</label>
                        <div class="col-sm-9">
                            <input type="text" name="LastName" ng-model="objAttorney.LastName" maxlength="20" placeholder="Last Name" class="form-control" required>
                        </div>
                    </div>
                </div>
                <div class="row mb-10">
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0">Phone No:</label>
                        <div class="col-sm-2">
                            <input type="text" name="PhoneCode" ng-model="objAttorney.AreaCode1" maxlength="5" placeholder="Code" integer-Only="" class="form-control">
                        </div>
                        <div class="col-sm-4">
                            <input type="text" name="PhoneNo" ng-model="objAttorney.PhoneNo" maxlength="10" placeholder="Phone No" integer-Only="" class="form-control">
                        </div>
                        <div class="col-sm-3 pr-0">
                            <ul class="borderless pt-0 pl-0">
                                <li class="li-checkbox col-sm-12 pl-0">
                                    <label class="lbl-checkbox">
                                        <input name="PhoneNoAsFirm" type="checkbox" ng-checked="objAttorney.SamePhone"> Same Phone as Firm
                                    </label>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0">Fax No:</label>
                        <div class="col-sm-2">
                            <input type="text" name="FaxCode" ng-model="objAttorney.AreaCode2" maxlength="4" placeholder="Code" integer-Only="" class="form-control">
                        </div>
                        <div class="col-sm-4">
                            <input type="text" name="FaxNo" ng-model="objAttorney.FaxNo" maxlength="10" placeholder="Fax No" integer-Only="" class="form-control">
                        </div>
                        <div class="col-sm-3 pr-0">
                            <ul class="borderless pt-0 pl-0">
                                <li class="li-checkbox col-sm-12 pl-0">
                                    <label class="lbl-checkbox">
                                        <input name="LinkedLocationgrp" type="checkbox" ng-checked="objAttorney.SamePhone"> Same Fax as Firm
                                    </label>
                                </li>
                            </ul>
                        </div>

                    </div>
                </div>
                <div class="row mb-10">
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0">State Bar No:</label>
                        <div class="col-sm-9">
                            <input type="text" name="StateBarNo" ng-model="objAttorney.StateBarNo" maxlength="15" placeholder="State Bar No." class="form-control">
                        </div>
                    </div>
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0">Email:</label>
                        <div class="col-sm-9">
                            <input type="email" name="Email" ng-model="objAttorney.Email" maxlength="200" placeholder="Email" class="form-control">
                        </div>
                    </div>
                </div>

                <div class="row mb-10">
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0">Attorney Type:</label>
                        <div class="col-sm-9">
                            <select name="AttyType" ng-model="objAttorney.AttyType" class="form-control" ng-options="item.Value as item.Title for item in  AttorneyTypeList"></select>
                        </div>
                    </div>
                    <div class="col-md-6" id="divAssistantContact">
                        <label class="col-sm-3 control-label lbl-primary pr-0">Assistant / Contact:</label>
                        <div class="col-sm-9 pl-0">
                            <div class="col-sm-12 pl-0 mb-5" ng-repeat="AssistantContact  in AttorneyAssistantContactList">
                                <input type="text" ng-model="AssistantContact.AdditionalContactsID" hidden />
                                <div class="col-sm-4">
                                    <input type="text" name="AssistantContact" ng-model="AssistantContact.AssistantName" maxlength="30" placeholder="Assistant / Contact Name" class="form-control">
                                </div>
                                <div class="col-sm-5">
                                    <input type="email" name="AssistantContactEmail{{$index}}" ng-model="AssistantContact.AssistantEmail" maxlength="200" placeholder="Assistant / Contact Email" class="form-control">
                                    <label class="alert-danger" ng-show="Attorneyform.AssistantContactEmail{{$index}}.$error.email && !Attorneyform.$valid && Attorneyform.$submitted">Invalid Assistant Email.</label>
                                </div>
                                <div class="col-sm-2">
                                    <button ng-show="$index==0 || AttorneyAssistantContactList.length==($index+1)" type="button" class="btn btn-primary btn-icon btn-rounded" ng-click="AddAssistantContact()">
                                        <i class="glyphicon glyphicon-plus"></i>
                                    </button>
                                    <button ng-show="$index!=0" type="button" class="btn btn-danger btn-icon btn-rounded" ng-click="RemoveAssistantContact(AssistantContact)">
                                        <i class="glyphicon glyphicon-minus"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row mb-10">
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0">Doc Cap for Records :</label>
                        <div class="col-sm-9">
                            <div class="input-group">
                                <span class="input-group-addon">$</span>
                                <input type="number" name="DocCapforRecords" ng-model="objAttorney.DocCapForRecords" ng-pattern="/^[0-9]+(\.[0-9]{1,2})?$/" step="0.50" maxlength="4" max="9999" class="form-control">
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0">Doc Cap for Films :</label>
                        <div class="col-sm-9">
                            <div class="input-group">
                                <span class="input-group-addon">$</span>
                                <input type="number" name="DocCapforFilms" ng-model="objAttorney.DocCapForFilms" ng-pattern="/^[0-9]+(\.[0-9]{1,2})?$/" step="0.50" maxlength="4" max="9999" class="form-control">
                            </div>
                        </div>
                    </div>
                </div>


                <div class="row mb-10">
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0">Document Production Preference</label>
                        <div class="col-sm-9">
                            <div class="bx-grey pb-5 pr-5 col-sm-12" style="min-height:201px;">
                                <ul class="borderless pt-0 pl-0">
                                    <li class="li-checkbox col-sm-4 pl-0" ng-repeat="item in DocumentProdctionOptions">
                                        <label class="lbl-checkbox bg-white">
                                            <input name="documentProdctionOptionsgrp" type="radio" ng-click="RadioCheckChange(item, DocumentProdctionOptions)" data-ng-model="item.IsChecked" data-ng-value="true">{{item.Title}}
                                        </label>
                                    </li>
                                </ul>
                            </div>
                        </div>

                    </div>
                    <div class="col-md-6">
                        <div class="row no-margin mb-10">
                            <label class="col-sm-3 control-label lbl-primary pr-0"></label>
                            <div class="col-sm-9">
                                <div class="bx-grey pb-5 pr-5 col-sm-12">
                                    <ul class="borderless pt-0 pl-5">
                                        <li class="li-checkbox col-sm-6 pl-0">
                                            <label class="lbl-checkbox bg-white">
                                                <input name="HaveOwnAuthorization" type="checkbox" ng-click="OpenAddForms($event,1)" data-ng-model="objAttorney.OwnOrganization" /> Have Own Authorization
                                            </label>
                                        </li>
                                        <li class="li-checkbox col-sm-6 pl-0">
                                            <label class="lbl-checkbox bg-white">
                                                <input name="HaveOwnFaceSheet" type="checkbox" ng-click="OpenAddForms($event,2)" data-ng-model="objAttorney.OwnFacesheet">Have Own Face Sheet
                                            </label>
                                        </li>

                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="row no-margin mb-10">
                            <label class="col-sm-3 control-label lbl-primary pr-0"></label>
                            <div class="col-sm-9">
                                <div class="bx-grey pb-5 pr-5 col-sm-12">
                                    <ul class="borderless pt-0 pl-5">

                                        <li class="li-checkbox col-sm-6 pl-0">
                                            <label class="lbl-checkbox bg-white">
                                                <input name="HaveOwnNotice" type="checkbox" ng-click="OpenAddForms($event,3)" data-ng-model="objAttorney.OwnNotice">Have Own Notice
                                            </label>
                                        </li>
                                        <li class="li-checkbox col-sm-6 pl-0">
                                            <label class="lbl-checkbox bg-white">
                                                <input name="LocationSpecificRequests" type="checkbox" ng-click="OpenAddForms($event,4)" data-ng-model="objAttorney.LinkRequest">Location Specific Requests
                                            </label>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="row no-margin mb-10">
                            <label class="col-sm-3 control-label lbl-primary pt-0">Monthly Billing:</label>
                            <div class="col-sm-9 pr-0">
                                <div class="col-sm-12">
                                    <ul class="borderless pt-0 pl-0">
                                        <li class="li-checkbox col-sm-6 pl-0">
                                            <label class="lbl-checkbox">
                                                <input name="MonthlyBillingGroup" data-ng-model="objAttorney.MonthlyBilling" data-ng-value="true" type="radio"> Yes
                                            </label>
                                        </li>
                                        <li class="li-checkbox col-sm-6 pl-0">
                                            <label class="lbl-checkbox">
                                                <input name="MonthlyBillingGroup" data-ng-model="objAttorney.MonthlyBilling" data-ng-value="false" type="radio"> No
                                            </label>
                                        </li>
                                    </ul>
                                </div>
                                <div class="col-sm-12 pl-0 pr-0" ng-show="objAttorney.MonthlyBilling">
                                    <div class="col-sm-6">
                                        <input type="text" name="ContactName" ng-model="objAttorney.ContactName" maxlength="50" placeholder="Contact Name" class="form-control">
                                    </div>
                                    <div class="col-sm-6 pl-0 pr-20">
                                        <input type="email" name="ContactEmail" ng-model="objAttorney.ContactEmail" maxlength="100" placeholder="Contact Email" class="form-control">
                                        <label class="alert-danger" ng-show="Attorneyform.ContactEmail.$error.email && !Attorneyform.$valid && Attorneyform.$submitted">Invalid Contact Email.</label>
                                    </div>

                                </div>

                            </div>

                        </div>

                    </div>
                </div>
                <div class="row mb-10">
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pt-0">Preferred Contact:</label>
                        <div class="col-sm-9">

                            <div class="bx-grey pb-5 pr-5 col-sm-12">
                                <ul class="borderless pt-0 pl-0">
                                    <li class="li-checkbox col-sm-4 pl-0" ng-repeat="item in PreferredContactOption">
                                        <label class="lbl-checkbox bg-white"><input name="PreferredGrp" type="checkbox" data-ng-model="item.IsChecked" ng-click="RadioCheckChange(item, PreferredContactOption)" data-ng-value="true"> {{item.Titile}}</label>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pt-0">Request Sent:</label>
                        <div class="col-sm-9">
                            <div class="bx-grey pb-5 pr-5 col-sm-12">
                                <ul class="borderless pt-0 pl-0">
                                    <li class="li-checkbox col-sm-4 pl-0" ng-repeat="item in RequestSentOptions">
                                        <label class="lbl-checkbox bg-white"><input name="RequestSentgrp" type="radio" data-ng-model="item.IsChecked" ng-click="RadioCheckChange(item, RequestSentOptions)" data-ng-value="true"> {{item.Title}}</label>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>

                </div>
                <div class="row mb-10">
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0">Disable this Attorney:</label>
                        <div class="col-sm-9">
                            <ul class="borderless pt-0 pl-0">
                                <li class="li-checkbox col-sm-12 pl-0 pr-0">
                                    <label class="lbl-checkbox">
                                        <input name="DisableAttorney" type="checkbox" data-ng-model="objAttorney.isDisable"> Disable Attorney
                                    </label>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="row mb-10">
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0">Client Of</label>
                        <div class="col-sm-2 display-inline-flex">
                            <input name="ClientOf" type="text" class="form-control" ng-model="objAttorney.ClientOf" readonly="readonly">
                            <a class="ico_btn cursor-pointer mt-1" ng-click="ShowEmployeePopup('client')" title="Search Employee"> <i class="icon-search4"></i></a>
                        </div>
                        <div class="col-sm-4 display-inline-flex">
                            <input name="SalesRep" type="text" class="form-control" ng-model="objAttorney.ClientOfFirstName" readonly="readonly">
                        </div>
                        <div class="col-sm-3 display-inline-flex">
                            <input name="SalesRep" type="text" class="form-control" ng-model="objAttorney.ClientOfLastName" readonly="readonly">
                        </div>
                    </div>
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0">Sales Rep</label>
                        <div class="col-sm-2 display-inline-flex">
                            <input name="SalesRep" type="text" class="form-control" ng-model="objAttorney.SalesRep" readonly="readonly">
                            <a class="ico_btn cursor-pointer mt-1" ng-click="ShowEmployeePopup('sales')" title="Search Employee"> <i class="icon-search4"></i></a>
                        </div>
                        <div class="col-sm-4 display-inline-flex">
                            <input name="SalesRep" type="text" class="form-control" ng-model="objAttorney.SalesRepFirstName" readonly="readonly">
                        </div>
                        <div class="col-sm-3 display-inline-flex">
                            <input name="SalesRep" type="text" class="form-control" ng-model="objAttorney.SalesRepLastName" readonly="readonly">
                        </div>
                    </div>

                </div>
                <div class="row  mb-10">
                    <div class="col-md-6">
                        <label class="col-sm-3 control-label lbl-primary pr-0">Warning</label>
                        <div class="col-sm-9">
                            <textarea class="form-control" ng-model="objAttorney.Warning" name="Warning" rows="4" maxlength="256" style="resize:none"> </textarea>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="col-sm-3">
                            <div class="col-sm-6">
                                <label class="control-label lbl-primary pr-0">Notes</label>
                            </div>
                            <div class="col-sm-6 text-right pt-30 pr-0">
                                <i class="icon-clipboard3 position-left cursor-pointer" title="Click here to view notes" ng-click="openAttorneyNoteClick();"></i>
                            </div>
                        </div>
                        <div class="col-sm-9">
                            <textarea class="form-control" id="Notes" name="Notes" rows="4" maxlength="1000" style="resize:none"> </textarea>
                        </div>
                    </div>
                </div>
                <div class="row mb-10">
                    <div class="col-md-12 text-right" style="padding-right:17px;">
                        <button type="button" ui-sref="AttorneyDetail" class="btn btn-default" title="Cancel">Cancle</button>
                        <button type="submit" title="Save" ng-if="(IsUserCanEditAttorney && isEdit) || (IsUserCanAddAttorney && isEdit==false)" class="btn btn-primary" ng-click="SaveAttorney(Attorneyform)">Save</button>
                    </div>
                </div>
            </form>

        </div>
    </div>

    <div id="modal_Employee" class="modal fade" data-backdrop="static" data-keyboard="false">
        <form class="form-horizontal" name="frmEmployee" novalidate>
            <div class="modal-dialog modal-lg">
                <div class="modal-content">
                    <div class="modal-header bg-main">
                        @*<button type="button" class="close" data-dismiss="modal">&times;</button>
                            <h6 class="modal-title" ng-bind="modal_Title"></h6>*@
                        <button type="button" class="close" data-dismiss="modal" title="Close">&times;</button>
                        <i class="icon-user pull-left mr-10 mt-5"></i> <h6 class="modal-title" ng-bind="EmployeeTitle"></h6>
                    </div>
                    <div class="modal-body">
                        <div class="modal-container">
                            <div class="table-responsive table-grid">
                                <table class="table table-bordered table-framed" id="tblEmployee"></table>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal" title="Close">Close</button>
                    </div>
                </div>
            </div>
        </form>
    </div>

    <div id="modal_FirmSearch" data-backdrop="static" data-keyboard="false" class="modal fade" static-Include="./Templates/Settings/Partial/FirmSearch.cshtml" ng-controller="FirmSearchController">
    </div>

    <div id="modal_AdditionalContacts" class="modal fade" data-backdrop="static" data-keyboard="false">
        <form class="form-horizontal" name="frmAdditionalContacts" novalidate>
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header bg-main">
                        @*<button type="button" class="close" data-dismiss="modal">&times;</button>
                            <h6 class="modal-title" ng-bind="modal_Title"></h6>*@
                        <button type="button" class="close" data-dismiss="modal" title="Close">&times;</button>
                        <i class="icon-file-plus pull-left mr-10 mt-5"></i> <h6 class="modal-title">Add Additional Contacts</h6>
                    </div>
                    <div class="modal-body">
                        <div class="modal-container">
                            <div id="messages1">
                                <div class="alert alert-danger alert-dismissible validationErrors" ng-show="(frmAdditionalContacts.$submitted && !frmAdditionalContacts.$valid)">
                                    <ul>
                                        <li ng-show="frmAdditionalContacts.FirstName.$error.required">Please enter First Name.</li>
                                        <li ng-show="frmAdditionalContacts.LastName.$error.required">Please enter Last Name.</li>
                                        <li ng-show="frmAdditionalContacts.Email.$error.required">Please enter Email.</li>
                                        <li ng-show="frmAdditionalContacts.Email.$error.email">Please enter valid Email.</li>
                                    </ul>
                                </div>
                            </div>

                            <div class="row mb-10">
                                <label class="col-sm-2 control-label lbl-primary">First Name</label>
                                <div class="col-sm-8">
                                    <input type="text" id="FirstName" class="form-control" name="FirstName" ng-model="objAdditionalContacts.FirstName" maxlength="50" required />
                                </div>
                            </div>

                            <div class="row mb-10">
                                <label class="col-sm-2 control-label lbl-primary">Last Name</label>
                                <div class="col-sm-8">
                                    <input type="text" class="form-control" name="LastName" ng-model="objAdditionalContacts.LastName" maxlength="50" required />
                                </div>
                            </div>
                            <div class="row mb-10">
                                <label class="col-sm-2 control-label lbl-primary">Email</label>
                                <div class="col-sm-8">
                                    <input type="email" class="form-control" name="Email" ng-model="objAdditionalContacts.Email" maxlength="200" required />
                                </div>
                            </div>
                            <div class="table-responsive table-grid">
                                <table class="table table-bordered table-framed" id="tblAdditiionalContacts">
                                    <thead>
                                        <tr role="row">
                                            <th class="dt-left sorting_disabled" tabindex="0" rowspan="1" colspan="1" style="width:100px;">First Name</th>
                                            <th class="dt-left sorting_disabled" tabindex="0" rowspan="1" colspan="1" style="width:100px;">Last Name</th>
                                            <th class="dt-left sorting_disabled" tabindex="0" rowspan="1" colspan="1" style="width:200px;">Email</th>
                                            <th class="dt-left sorting_disabled" tabindex="0" rowspan="1" colspan="1" style="width:50px;">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr class="odd" ng-repeat="item in AdditionalContactsList">
                                            <td>{{item.FirstName}}</td>
                                            <td>{{item.LastName}}</td>
                                            <td>{{item.Email}}</td>
                                            <td><a class='ico_btn cursor-pointer' ng-click='DeleteAdditionalContact(item.ContactID)' title='Delete'>  <i class='icon-trash cursor-pointer'></i></a></td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>

                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal" title="Close">Close</button>
                        <button type="submit" ng-click="SaveAdditionalContacs(frmAdditionalContacts)" class="btn btn-primary" title="Save">Save</button>
                    </div>
                </div>
            </div>
        </form>
    </div>

    <div id="modal_LocationFormList" data-backdrop="static" data-keyboard="false" class="modal fade" static-Include="./Templates/Settings/Partial/ManageFormAndRequestedForm.cshtml" ng-controller="ManageFormAndRequestedFormController">
    </div>
    <div id="modal_NotesForm" class="modal fade" static-Include="./Templates/Settings/Partial/Notes.cshtml">
    </div>
</div>